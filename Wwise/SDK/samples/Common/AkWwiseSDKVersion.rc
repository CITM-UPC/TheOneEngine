            VALUE "ProductName", "Wwise"
            VALUE "ProductVersion", AK_WWISESDK_VERSION_STRING
#if defined(_DEBUG)
            VALUE "Build Configuration", "Debug"
#elif defined(_PROFILE)
            VALUE "Build Configuration", "Profile"
#endif            
            VALUE "Build Number", AK_WWISESDK_NUM2STRING( AK_WWISESDK_VERSION_BUILD )
            VALUE "Version Name", AK_WWISESDK_VERSIONNAME
        END
    END
    BLOCK "VarFileInfo"
