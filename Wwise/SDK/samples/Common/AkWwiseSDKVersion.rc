	AK_WWISESDK_NUM2STRING( AK_WWISESDK_VERSION_MAJOR ) \
	"." AK_WWISESDK_NUM2STRING( AK_WWISESDK_VERSION_MINOR ) \
	"." AK_WWISESDK_NUM2STRING( AK_WWISESDK_VERSION_SUBMINOR ) \
	"." AK_WWISESDK_NUM2STRING( AK_WWISESDK_VERSION_BUILD )

VS_VERSION_INFO VERSIONINFO
 FILEVERSION AK_WWISESDK_VERSION_MAJOR,AK_WWISESDK_VERSION_MINOR,AK_WWISESDK_VERSION_SUBMINOR,AK_WWISESDK_VERSION_BUILD
